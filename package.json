{
  "name": "svg-scaler-loader",
  "version": "0.0.0",
  "description": "Webpack svg scale loader",
  "keywords": [
    "webpack",
    "loader",
    "utils",
    "svg",
    "scale",
    "resize",
    "svgo"
  ],
  "scripts": {
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build": "npm run build:es && npm run build:cjs",
    "release": "standard-version",
    "lint": "eslint . --ext .js",
    "lint:update": "export PKG=eslint-config-airbnb-base; npm info \"$PKG@latest\" peerDependencies --json | command sed 's/[{},]//g ; s/: /@/g' | xargs yarn add --dev \"$PKG@latest\"",
    "precommit": "lint-staged",
    "security": "nsp check",
    "co": "git-cz"
  },
  "lint-staged": {
    "*.js": [
      "eslint --format=codeframe --fix",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com:madzhup/svg-scaler-loader.git"
  },
  "bugs": {
    "url": "https://github.com/madzhup/svg-scaler-loader/issues"
  },
  "homepage": "https://github.com/madzhup/svg-scaler-loader",
  "author": "madzhup <madzhup@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "loader-utils": "^1.1.0",
    "schema-utils": "^0.4.5",
    "svg-scaler-viewbox": "^0.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.52",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "@babel/preset-stage-0": "^7.0.0-beta.52",
    "babel-eslint": "^8.2.5",
    "commitizen": "^2.10.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "nsp": "^3.2.1",
    "prettier": "^1.13.7",
    "webpack": "^4.16.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
